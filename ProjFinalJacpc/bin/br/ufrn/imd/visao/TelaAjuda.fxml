<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.ufrn.imd.controle.TelaAjudaController">
   <children>
      <VBox alignment="CENTER" prefHeight="300.0" prefWidth="800.0">
         <children>
            <TextArea fx:id="taInstrucoes" prefHeight="250.0" prefWidth="800.0" />
            <Button fx:id="btnSair" mnemonicParsing="false" onAction="#sair" text="Sair" />
         </children>
      </VBox>
   </children>
</AnchorPane>
